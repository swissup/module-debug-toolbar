<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <link src="Mgt_DeveloperToolbar::js/highlight.min.js"/>
    </head>
    <body>
        <referenceBlock name="breeze.js">
            <arguments>
                <argument name="bundles" xsi:type="array">
                    <item name="debug" xsi:type="array">
                        <item name="active" xsi:type="boolean">true</item>
                        <item name="items" xsi:type="array">
                            <item name="mgtdevelopertoolbar" xsi:type="string">Mgt_DeveloperToolbar/js/toolbar</item>
                            <item name="Mgt_DeveloperToolbar/js/blocks" xsi:type="string">Mgt_DeveloperToolbar/js/blocks</item>
                            <item name="Mgt_DeveloperToolbar/js/events" xsi:type="string">Mgt_DeveloperToolbar/js/events</item>
                            <item name="Mgt_DeveloperToolbar/js/plugins" xsi:type="string">Mgt_DeveloperToolbar/js/plugins</item>
                            <item name="Mgt_DeveloperToolbar/js/clipboard" xsi:type="string">Mgt_DeveloperToolbar/js/clipboard</item>
                            <item name="Mgt_DeveloperToolbar/js/treegrid" xsi:type="string">Mgt_DeveloperToolbar/js/breeze/treegrid</item>
                        </item>
                    </item>
                </argument>
            </arguments>
        </referenceBlock>
    </body>
</page>
